{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFCARSALESALI"
		},
		"LS_AZRSQL_SALES_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AZRSQL_SALES'"
		},
		"LS_AZRSQL_SALES_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlcarsales.database.windows.net"
		},
		"LS_AZRSQL_SALES_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqlserver"
		},
		"LS_AZRSQL_SALES_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminali"
		},
		"LS_GIT_AZRSQL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFCARSALESALI_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AZRSQL_SALES_password": {
						"value": "[parameters('LS_AZRSQL_SALES_password')]"
					},
					"LS_AZRSQL_SALES_properties_typeProperties_server": {
						"value": "[parameters('LS_AZRSQL_SALES_properties_typeProperties_server')]"
					},
					"LS_AZRSQL_SALES_properties_typeProperties_database": {
						"value": "[parameters('LS_AZRSQL_SALES_properties_typeProperties_database')]"
					},
					"LS_AZRSQL_SALES_properties_typeProperties_userName": {
						"value": "[parameters('LS_AZRSQL_SALES_properties_typeProperties_userName')]"
					},
					"LS_GIT_AZRSQL_properties_typeProperties_url": {
						"value": "[parameters('LS_GIT_AZRSQL_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}